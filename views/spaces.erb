    <h1>Book a Space</h1>
    <a href="/spaces/new">
    <input type="submit" value="List a Space">
    </a>

    <% @spaces.each do |space| %>

    <div>
    <a href="/spaces/<%= space.id %>">
	<h3> <%= space.id %>. <%= space.name %> </h3>
    </a>
    <p> <%= space.description %> </p>
    </div>
				
    <% end %>
